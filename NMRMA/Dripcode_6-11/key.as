package {	import flash.display.*;	import flash.events.*;	public class key extends MovieClip	{		var par:Object;		var keyVal:String;		var def:String;		public function key():void		{				buttonMode = true;			useHandCursor = true;			par = this.parent;			def = par.zip_field.text;			keyVal = name;			var result:Array = keyVal.split("_");			keyVal = result[1];			addEventListener(MouseEvent.MOUSE_UP,pressed)		}				private function pressed(e:MouseEvent):void		{						if(par.zip_field.text.length < 5)			{							if(par.zip_field.text == def){ 					par.zip_field.text = keyVal				}				else{					par.zip_field.text+= keyVal;				}			}			else if(par.zip_field.text.length == 5)			{				trace("exactly 5");			}						else par.tooMany();					}							}}