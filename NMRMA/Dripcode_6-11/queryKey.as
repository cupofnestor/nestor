package {	import flash.display.*;	import flash.events.*;	public class  queryKey extends MovieClip	{		var par:Object;		var current:String;		public function queryKey():void		{				buttonMode = true;			useHandCursor = true;			par = this.parent;			addEventListener(MouseEvent.MOUSE_UP,pressed)		}				private function pressed(e:MouseEvent):void		{						if(par.zip_field.text.length == 5){				var query:String = par.zip_field.text;				trace(par.name);				par.parent.loadMap(query);			}			else par.tooMany();					}							}}