package{	import flash.display.*;	import flash.events.*;	import caurina.transitions.*; 	public class  graph extends MovieClip	{				public function graph():void		{							addEventListener(Event.ADDED_TO_STAGE, birth);		}				private function birth(e:Event):void		{						removeEventListener(Event.ADDED_TO_STAGE, birth);			alpha=0;			//trace("graph added");			Tweener.addTween( this, {alpha:1, time:2, transition:"linear"})		}				public function kill():void		{			var suicide:Function = function suicide():void			{				//trace("graph suicide");				dispatchEvent(new Event("dying"));				var par:Object = this.parent;				par.removeChild(this);			}			//trace("graph killed");			Tweener.addTween( this, {alpha:0, time:2, transition:"easeInExpo", onComplete:suicide})		}			}}