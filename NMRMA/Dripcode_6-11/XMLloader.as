package{	import flash.display.*;	import flash.net.*;	import flash.events.*;	public class XMLloader extends Sprite		{		public var regions:XMLList = new XMLList();		var xml:XML;		public var d1:dispatcher = new dispatcher("XMLloader");						public function XMLloader(_file:String):void		{			var loader:URLLoader = new URLLoader();			loader.addEventListener(Event.COMPLETE, onLoaded);			loader.load(new URLRequest(_file));		}							private function onLoaded(e:Event):void		{			xml = new XML(e.target.data);			trace("xml test ");			trace(xml.map is XMLList);						regions = xml.map;			trace("Region: "+regions.toString());			trace(regions);			d1.loaded();		}				public function getDescription(_i:Number):String		{			var description:String = regions[_i].description			return description;		}	}		}