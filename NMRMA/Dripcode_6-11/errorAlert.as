package{	import flash.display.*;	import flash.events.*;	import caurina.transitions.*; 	import flash.utils.Timer;		public class  errorAlert extends MovieClip	{		private var par:Object;		private var countDown:Timer;		public function errorAlert():void		{				countDown = new Timer(4000, 1);						//			addEventListener(Event.ADDED_TO_STAGE, birth);		}				private function birth(e:Event):void		{			countDown.start();			countDown.addEventListener(TimerEvent.TIMER_COMPLETE, kill);			removeEventListener(Event.ADDED_TO_STAGE, birth);									this.scaleX=0;			this.scaleY=0;			this.alpha=0;			Tweener.addTween( this, { scaleX:1,scaleY:1, alpha:1, time:1, transition:"easeOutBack" } );		}				public function kill(e:TimerEvent):void		{						var suicide:Function = function suicide():void			{				dispatchEvent(new Event("dying"));				//trace("graph suicide");				var par:Object = this.parent;				par.removeChild(this);			}			countDown.stop();			countDown.reset();			//trace("alert killed");			Tweener.addTween( this, { scaleX:0,scaleY:0, alpha:0, time:1, transition:"easeOut", onComplete:suicide } );		}									}}