package{	import flash.display.*;	import flash.net.*;	import flash.events.*;	public class animation extends graph	{		var d1:dispatcher = new dispatcher("animation");		var btn:continueBtn;		public function animation():void		{			addEventListener(Event.ENTER_FRAME, popText);				}			function popText(e:Event):void		{			switch(e.target.currentFrame)			{				case 60:					d1.finished();					removeEventListener(Event.ENTER_FRAME, popText);					this.stop();					this.kill();					break;													case 124:										btnBirth();					break;								case 263:										btnBirth();					break;									case 337:										btnBirth();					break;									case 378:										btnBirth();					break;				case 440:										btnBirth();					break;				case 482:										btnBirth();					break;									case 509:					d1.finished();					break;									case 510:					removeEventListener(Event.ENTER_FRAME, popText);					this.stop();					break;					this.kill();								}							}				function btnBirth():void		{			btn = new continueBtn();			trace("btn");			this.stop();			addChild(btn);			btn.x = 1150;			btn.y=670;			removeEventListener(Event.ENTER_FRAME, popText);			btn.active_area.addEventListener(MouseEvent.MOUSE_UP,btnDeath);			this.dispatchEvent(new Event("stopped"));		}				function btnDeath(e:Event):void		{			addEventListener(Event.ENTER_FRAME, popText);			btn.active_area.removeEventListener(MouseEvent.MOUSE_UP,btnDeath);			removeChild(btn);			play();			this.dispatchEvent(new Event("continue"));					}	}}		