package {	import flash.display.*;	import flash.events.*;	public class  deleteKey extends MovieClip	{		var par:Object;		var current:String;		public function deleteKey():void		{				buttonMode = true;			useHandCursor = true;			par = this.parent;			addEventListener(MouseEvent.MOUSE_UP,pressed)		}				private function pressed(e:MouseEvent):void		{			current = par.zip_field.text;			par.zip_field.text = current.slice(0, current.length - 1);					}							}}