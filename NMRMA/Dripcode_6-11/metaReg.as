package {	import flash.display.*;	import flash.events.*;			public class  metaReg extends graph	{		public var d1:dispatcher = new dispatcher("meta");		public var regDescription:regDes = new  regDes();		var par:Object;				public function metaReg():void				{												}				public function loadReg(reg:String):void		{			var killLoader:Function = function killLoader(e:Event):void			{				trace('reg2 loaded');				loaderClip.kill();				//var hucTextLayer_mc =  new hucTextLayer();				//addChild(hucTextLayer_mc);				window_mc.addChild(regMap);								meta_text.label.text = "Watershed Region:";				meta_text.detail.field.text = desc;				meta_text.hucField.text =  regMap.reg_name;				hucLabel.field.text = "#"+regMap.regCode;				//reg_name = zipMap.reg_name;								d1.loaded();							}						this.dispatchEvent(new Event("query"));			var loaderClip:MovieClip = new loaderAnim2();			meta_text.label.text = "";			meta_map.kill();			d1.doLoad();			this.addChildAt(loaderClip,0);			reg = reg;			var desc:String = regDescription.getDesc(Number(reg)-1);			this.addChildAt(loaderClip,0);															var hucLabel:hucBlank = new hucBlank();			addChild(hucLabel);			hucLabel.label.text = "Watershed Region"			//var mask_mc:MovieClip = new glassMask();			//stage.addChildAt(mask_mc,1);						var regMap:regLoader2 = new regLoader2(reg);						regMap.dLoaded.addEventListener(dispatcher.LOADED,killLoader);						trace("regLoaded");		}					}}