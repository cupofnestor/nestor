package{	import flash.text.*;	import flash.display.MovieClip;	import flash.events.*;	public class keypad extends graph		{		var titleFont:Font;		var fieldFont:Font;				public function keypad():void		{				go.visible = true;			titleFont = new Title();						/* Create a new TextFormat object, and set the font property to the myFont			   object's fontName property. */			var titleFormat:TextFormat = new TextFormat();			titleFormat.font = titleFont.fontName;			titleFormat.size = 36;			/* Create a new TextField object, assign the text format using the 			   defaultTextFormat property, and set the embedFonts property to true. */			label_field.autoSize = TextFieldAutoSize.LEFT;			label_field.defaultTextFormat = titleFormat;			label_field.embedFonts = true;									/****************************************************/					fieldFont = new h1();						/* Create a new TextFormat object, and set the font property to the myFont			   object's fontName property. */			var fieldFormat:TextFormat = new TextFormat();			fieldFormat.font = fieldFont.fontName;			fieldFormat.size = 33;						/* Create a new TextField object, assign the text format using the 			   defaultTextFormat property, and set the embedFonts property to true. */						zip_field.autoSize = TextFieldAutoSize.LEFT;			zip_field.defaultTextFormat = fieldFormat;			zip_field.embedFonts = true;					}				public function tooMany():void		{			dispatchEvent(new Event("tooMany"));		}				public function enterKey():void		{			go.visible = false;		}	}		}