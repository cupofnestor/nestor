package com.nestor.sxc.photoop  {		import flash.display.MovieClip;			public class photoop extends MovieClip {						public function photoop() {			knobby.addEventListener(MouseEvent.MOUSE_UP, three);		}				private function three(){			trace("THREE");			count3.addEventListener(Event.COMPLETE, two);			count3.blink();		}				private function two(){			trace("knob pushed");			count3.removeEventListener(Event.COMPLETE, two);			count2.addEventListener(Event.COMPLETE, one);			count2.blink();		}		private function one(){			trace("knob pushed");			count2.removeEventListener(Event.COMPLETE, two);			count1.addEventListener(Event.COMPLETE, go);			count1.flash();		}				private function go(){			trace("GO!");					}							}	}