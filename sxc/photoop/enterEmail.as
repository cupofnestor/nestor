package com.nestor.sxc.photoop {		import flash.display.MovieClip;	import flash.events.KeyboardEvent;	import flash.text.TextField;	import flash.events.Event;	import flash.display.MovieClip;	import com.greensock.TweenLite;		public class enterEmail extends MovieClip {						public function enterEmail() {			addEventListener(Event.ADDED_TO_STAGE,setup);			addEventListener(KeyboardEvent.KEY_UP,keypressed);			addEventListener("functionKey",funKeypressed);			onStage();		}		function keypressed(e:KeyboardEvent):void		{			var key:String = String.fromCharCode(e.charCode);			trace("key pressed:"+e.charCode);			address.text +=key;		}		function funKeypressed(e:Event):void		{			var key:String = e.target.val;			trace("fun key pressed:"+key);			if(key=="del") address.text = address.text.substr(0,address.length-1)		}		private function setup(e:Event):void{			rotation=-180;		}						public function offStage() {			TweenLite.to(this,3,{rotation:-180});		}				public function onStage() {			TweenLite.to(this,3,{rotation:0});		}			}	}/**/