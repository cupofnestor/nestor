package com.nestor.sxc.photoop  {	import flash.events.MouseEvent;	import flash.events.Event;	import flash.display.MovieClip;			public class photo_main extends MovieClip {						public function photo_main() {			knobby.addEventListener(MouseEvent.MOUSE_UP, three);		}				private function three(e:Event){			trace("THREE");			count3.addEventListener(Event.COMPLETE, two);			count3.blink();		}				private function two(e:Event){			trace("knob pushed");			count3.removeEventListener(Event.COMPLETE, two);			count2.addEventListener(Event.COMPLETE, one);			count2.blink();		}		private function one(e:Event){			trace("knob pushed");			count2.removeEventListener(Event.COMPLETE, two);			count1.addEventListener(Event.COMPLETE, go);			count1.flash();		}				private function go(e:Event){			trace("GO!");					}							}	}