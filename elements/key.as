package com.nestor.elements {		import flash.display.MovieClip;	import flash.events.*;	import com.greensock.TweenMax;	import com.greensock.easing.*;		public class key extends MovieClip {		[Inspectable]	public var val:String;				public function key() {			loaderInfo.addEventListener(Event.INIT, onInit);		}				public function onInit(e:Event) {		// now we can use component parameters!		trace("MY Value:"+val); // prints the param value		this.labelText.text=val;		labelText.addEventListener(MouseEvent.MOUSE_UP, pressed);	  }	  	  private function pressed(m:MouseEvent):void	  {		  trace(val+" Key Pressed");		  TweenMax.to (this, 0.2, {repeat:1, z:-5, yoyo:true, ease:Quad.easeInOut, rotationX:5});		  sendEvent();		 						  		  	  }	  	  private function sendEvent():void	  {		  var char:uint = val.charCodeAt(0);		  var evt:KeyboardEvent=new KeyboardEvent(KeyboardEvent.KEY_UP,true,true,char);		  dispatchEvent(evt);	  }			}	}/*package com.nestor.test{import flash.display.MovieClip;import flash.events.*;	public class  extended extends MovieClip {		  [Inspectable]	  public var myFancyComponentParameter:String;		  public function extended() {		// myFancyComponentParameter not set here yet		trace(myFancyComponentParameter); // prints null		loaderInfo.addEventListener(Event.INIT, onInit);			  }		  public function onInit(e:Event) {		// now we can use component parameters!		trace("MY Param:"+myFancyComponentParameter); // prints the param value	  }		}}*/