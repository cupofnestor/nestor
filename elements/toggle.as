package com.nestor.elements {		import flash.display.MovieClip;	import flash.events.MouseEvent;	import flash.events.Event;		public class toggle extends MovieClip {		public var toggleState:Boolean=false;				public function toggle() {			this.addEventListener(MouseEvent.MOUSE_UP, clicked);		}				public function clicked(m:MouseEvent):void		{			if(!toggleState){				toggleState=true;				gotoAndStop("down")				dispatchEvent(new Event(Event.CHANGE));			}			else			{				toggleState=false;				gotoAndStop("up")				dispatchEvent(new Event(Event.CHANGE));			}		}					}	}