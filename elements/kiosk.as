package com.nestor.elements  {	import flash.display.Stage;	import flash.display.NativeWindow;	import flash.display.StageDisplayState;	import flash.desktop.NativeApplication;	import flash.ui.Mouse;	import flash.events.*;	import flash.display.MovieClip;	public class kiosk extends MovieClip{				public function kiosk() {						this.addEventListener(Event.ADDED,added);			stage.addEventListener(KeyboardEvent.KEY_UP,shortcuts);			NativeApplication.nativeApplication.addEventListener(Event.DEACTIVATE, notSoFast);		}				private function added(e:Event):void		{			stage.nativeWindow.x=0;			stage.nativeWindow.y=0;			/*if(e.target.onTop)goOnTop();			if(e.target.fullscreen)goFull();			if(!e.target.mouse)hideMouse();*/					}				private function notSoFast(e:Event):void		{			trace("Cahanggaa");			stage.nativeWindow.orderToFront();			stage.nativeWindow.activate();					}				private function goOnTop():void		{						stage.nativeWindow.alwaysInFront = true;			stage.addEventListener(FullScreenEvent.FULL_SCREEN, resetTop);					}				private function goOffTop():void		{			stage.nativeWindow.alwaysInFront = false;			stage.removeEventListener(FullScreenEvent.FULL_SCREEN, resetTop);					}				private function resetTop(e:Event):void		{						stage.nativeWindow.alwaysInFront = false;			stage.nativeWindow.alwaysInFront = true;								}		private function goFull():void		{						stage.displayState = StageDisplayState.FULL_SCREEN;			stage.nativeWindow.alwaysInFront = true;		}		private function hideMouse():void		{			stage.nativeWindow.activate();			stage.nativeWindow.orderToBack();			stage.nativeWindow.orderToFront();			Mouse.hide();		}				private function showMouse():void		{			stage.nativeWindow.activate();			stage.nativeWindow.orderToBack();			stage.nativeWindow.orderToFront();			Mouse.show();		}				private function shortcuts(k:KeyboardEvent):void		{			//109 = m, 102 = f			switch (k.charCode)			{				case 102://'f'//				goFull();				break;								case 104: //'h'//				hideMouse();				break;								case 115://'s'//				showMouse();				break;								case 116://'o'//				goOnTop();				break;								case 98://'o'//				goOffTop();				break;							}			trace(k.charCode);		}	}	}