package com.nestor.tools {	import flash.events.Event;	import flash.net.URLLoader;	import flash.net.URLRequest;	import flash.events.EventDispatcher;	public class cfgloader extends EventDispatcher{		public var cfg:XML;		public function cfgloader(url:String) {			var req:URLRequest = new URLRequest(url);			var loader:URLLoader = new URLLoader();			loader.addEventListener(Event.COMPLETE,loadcomplete);			loader.load(req);		}				function loadcomplete(e:Event):void		{			cfg = new XML(e.target.data);			trace(cfg);			this.dispatchEvent(new Event(Event.COMPLETE));		}	}	}