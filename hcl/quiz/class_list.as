package com.nestor.hcl.quiz {		import flash.display.MovieClip;	import flash.events.*;		public class class_list extends MovieClip {		private var classes:Array=[];				public function class_list(_classes:Array) {			// constructor code			classes=_classes;			parseHTML();					}				private function parseHTML():void		{			var HTMLtmp:String=""			for each (var s:String in classes){				HTMLtmp+="<a  href=\"event:"+s+"\" >"+s+"</a><br/>"										}			HTMLtmp+=""			htmlList.htmlText=HTMLtmp;			trace("HTMLTExt : "+HTMLtmp);						_container.height = htmlList.numLines*26;			this.addEventListener("link", linkPressed);		}		private function linkPressed(t:TextEvent):void		{			trace(t.text);		}					}	}