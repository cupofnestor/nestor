package com.nestor.hcl.quiz {		import flash.display.MovieClip;	import flash.events.*;		public class class_list extends MovieClip {		private var classes:Array=[];		public var reqI;		public function class_list(_classes:Array) {			// constructor code			classes=_classes;			parseHTML();					}				private function parseHTML():void		{			var i:Number=0			var HTMLtmp:String=""			for each (var s:String in classes){				HTMLtmp+="<a  href=\"event:"+i+"\" >"+s+"</a><br/>"				i++;							}			HTMLtmp+=""			htmlList.htmlText=HTMLtmp;			trace("HTMLTExt : "+HTMLtmp);						_container.height = htmlList.numLines*26;			this.addEventListener("link", linkPressed);		}		private function linkPressed(t:TextEvent):void		{			this.removeEventListener("link", linkPressed);			reqI=Number(t.text);			trace("linkPressed: "+reqI);			dispatchEvent(new Event("selectQuiz"));		}					}	}