package com.nestor.hcl.quiz {		import flash.display.MovieClip;	import flash.utils.Timer;	import flash.events.*;		public class warning extends MovieClip {				var i:Number=9;				public function warning() {			var t:Timer = new Timer(1000,9);			t.addEventListener(TimerEvent.TIMER, tick);			t.addEventListener(TimerEvent.TIMER_COMPLETE, done);			t.start();			okbtn.addEventListener(MouseEvent.MOUSE_UP, cancel);		}				private function tick(t:Event):void		{			i-=1;			trace("tick");			count.text=i.toString();					}		private function done(t:Event):void		{			dispatchEvent(new Event("timeout"));		}				private function cancel(m:Event):void		{			trace("CANCEL");			dispatchEvent(new Event("cancelled"));		}	}	}