package com.nestor.hcl.slideshow {	import flash.events.*;	import flash.display.Loader;	import flash.net.URLRequest;	import flash.utils.getDefinitionByName;		public class fingerprint extends tweenable{		var ld:Loader;		private var c:XML;				public function fingerprint(_c:XML) {			c=_c;			var s:String = c.topics.topic.(@id == 1).swf.@file			var btnC:XML = new XML( c.topics.topic.(@id == 1).btn);						setupBtn(btnC);						addEventListener(Event.ADDED_TO_STAGE, init);						ld= new Loader();			var rq:URLRequest = new URLRequest(s);			ld.load(rq);			ld.contentLoaderInfo.addEventListener(Event.COMPLETE, loadDone);					}				private function setupBtn(c:XML):void		{			var type:String=c.@type.toString();			var classObj:Object = getDefinitionByName( type );			var btnR = new classObj();			btnR.x=Number(c.@RX);			btnR.y=Number(c.@Y);			addChild(btnR);			btnR.btnMode="done";			btnR.addEventListener(MouseEvent.MOUSE_UP, btnHandler);		}				private function loadDone(e:Event):void		{			addChildAt(e.target.loader,0);			show();		}				private function init(e:Event):void		{			//trace("INIT");			alpha=0;					}		private function btnHandler(m:Event):void		{			dispatchEvent(new Event("removeME"));		}		public function born():void		{			this.show()		}		public function die():void		{			this.hide()		}					}	}