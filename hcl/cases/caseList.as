package com.nestor.hcl.cases {		import flash.display.MovieClip;	import flash.display.DisplayObject;	import flash.events.*;	import flash.geom.Point;	import caurina.transitions.*;		public class caseList extends MovieClip {		var offstage:Point=new Point(-500, -500);		var initroZ:Number = 90;		var dID:int;		var cases:Array = new Array();				public function caseList(_d:Array) {			this.rotation=initroZ			this.x=offstage.x;			this.y=offstage.y;			this.addEventListener(Event.ADDED_TO_STAGE, addedMe);			init(_d);		}								private function addedMe(e:Event):void		{					}				public function swoopOn():void		{			swoop(0,0,0);			//postIt.play();		}						public function swoopOff():void		{			this.swoop(offstage.x,offstage.y,initroZ);		}				private function swoop(newX:Number,newY:Number,newRoZ:Number):void		{			Tweener.addTween(this,{x:newX,y:newY,time:1,rotation:newRoZ});		}				private function init(d:Array):void		{			var aCaseList:Array=new Array();			var links:Array=new Array();						aCaseList = d;						for (var i in aCaseList)			{				var item:String = aCaseList[i][1];				var id:String = aCaseList[i][0];				trace("item: "+item);				var caseItem_mc:caseItem=new caseItem(item,int(id));				cases[i]=caseItem_mc;				list_mc.addChild(cases[i]);				caseItem_mc.addEventListener(MouseEvent.MOUSE_UP, linkClicked);				caseItem_mc.y=caseItem_mc.height*i;				caseItem_mc.hi_lite.visible=false;			}					}						function linkClicked(t:MouseEvent):void		{			dID=t.currentTarget.myId;			dispatchEvent(new Event("swapItems"));			for each(var i in cases)			{				if (i == t.currentTarget)				{					i.hi_lite.visible=true;				}				else				{					i.hi_lite.visible=false;				}						}		}			}	}