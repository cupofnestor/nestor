package  {		import flash.display.MovieClip;	import caurina.transitions.*;		public class item_flag extends MovieClip {		var initX:Number = 516;		var initY:Number = 478;		var initRoZ:Number = 0;		var offX:Number = initX+400;		var offY:Number = initY + 400;		var offRoZ:Number = -66;				public function item_flag() 		{			this.x=offX;			this.y=offY;			this.rotation=offRoZ;		}		public function swoopOn():void		{			swoop(initX,initY,initRoZ);		}		public function swoopOf():void		{			swoop(offX,offY,offRoZ);		}						private function swoop(tx:Number, ty:Number, tRoZ:Number):void		{			Tweener.addTween(this, {x:tx, y:ty, rotation:tRoZ, time:1});		}					}	}