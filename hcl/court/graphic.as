package com.nestor.hcl.court {	import caurina.transitions.*;	import flash.display.MovieClip;	import flash.events.Event;	public class graphic extends MovieClip	{		public function graphic(initAlpha:Number=1) {			this.alpha=initAlpha;					}				public function opacity(setME:Number):void		{			Tweener.addTween(this, {alpha:setME, time:1, transition:"easeOutSine", onComplete:dispatcher});		}				private function dispatcher():void		{			dispatchEvent(new Event("doneOpacityChange",true));		}			}	}