package  com.nestor.hcl.court {	import flash.display.MovieClip;	import flash.net.*;	import flash.events.*;	import flash.display.Loader;		public class listItem extends MovieClip	{		var db_id:Number=0;		var db_title:Number=1;		var db_file:Number=2;		var db_length:Number=3;		var db_abs:Number=4;		var db_desc:Number=5;		var db_thumb:Number=6;		var db_image:Number=7;		var db_position:Number=8;		var db_active:Number=9;		var clipId:Number;		var host:String;		var path:String;		var toc:Array;		var image:String;		var vid:String;		var clipTitle:String;		var desc:String;		var largo:String;				public function listItem(_toc:Array) {			toc=_toc;			addEventListener(Event.ADDED_TO_STAGE, init);			this.addEventListener(MouseEvent.MOUSE_UP, clicked);		}				private function init(e:Event):void		{			loadThumb();			this.title.text=toc[db_title];			this.duration.text=toc[db_length]+" minutes.";			this.abs.text=toc[db_abs];			largo=toc[db_length];			clipId=toc[db_id];			image="http://"+host+path+toc[db_image];			vid="http://"+host+path+toc[db_file];			clipTitle=toc[db_title];			desc=toc[db_desc];								}				private function loadThumb():void		{			var thURL:String="http://"+host+path+toc[db_thumb];			trace(thURL);			var thURLreq:URLRequest=new URLRequest(thURL);			var thLoader:Loader=new Loader();			thLoader.load(thURLreq);			frame_mc.addChild(thLoader);					}				private function clicked(e:MouseEvent):void				{					dispatchEvent(new Event("listClicked",true));				}	}	}