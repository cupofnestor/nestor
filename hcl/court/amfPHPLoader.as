package{import flash.display.MovieClip;import flash.net.*;import flash.events.*;	public class amfPHPLoader extends MovieClip	{		var gw:NetConnection = new NetConnection();	var res:Responder;	var d:Array;			public function amfPHPLoader(server:String)		{			res = new Responder(onResult, onFault);			gw.connect(server);					}				public function goGet(method:String):void		{			gw.call(method, res);		}				function onResult(responds:Object):void		{						trace(responds);			// parse the ArrayCollection			d = responds.serverInfo.initialData;			dispatchEvent(new Event(Event.COMPLETE));		}				function onFault(responds:Object):void		{			for(var i in responds)			{				trace(responds[i]);			}		}			}}